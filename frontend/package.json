{
  "name": "goalsight-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "react-scripts test --coverage --watchAll=false --passWithNoTests",
    "eject": "react-scripts eject",
    "test:api": "jest --config=jest.config.api.js",
    "test:components": "jest --config=jest.config.components.js",
    "test:all": "npm run test:api && npm run test:components",
    "test:e2e": "npx cypress run",
    "test:coverage:components": "jest --config=jest.config.components.js --coverage",
    "test:coverage:api": "jest --config=jest.config.api.js --coverage",
    "test:coverage:all": "npm run test:coverage:api && npm run test:coverage:components"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "autoprefixer": "^10.4.14",
    "cypress": "^14.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "msw": "^2.10.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.0"
  }
}
